@model IEnumerable<Proyecto_Final.Models.dbModels.Evento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TituloEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContenidoEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstatusEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEvento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Foto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TituloEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContenidoEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstatusEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaEvento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Foto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdEvento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdEvento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdEvento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
