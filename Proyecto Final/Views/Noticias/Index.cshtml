@model IEnumerable<Proyecto_Final.Models.dbModels.Noticia>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TituloNoticia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContenidoNoticia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoticiaStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNoticia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Foto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TituloNoticia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContenidoNoticia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoticiaStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNoticia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Foto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNoticia">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNoticia">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNoticia">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
